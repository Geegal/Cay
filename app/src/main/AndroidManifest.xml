        <activity
            android:name=".DestinationActivity"
            android:exported="false"
            android:label="@string/title_activity_destination"
            android:theme="@style/Theme.CayGee" />
        <activity
            android:name=".formActivity"
            android:exported="false"
            android:label="@string/title_activity_form"
            android:theme="@style/Theme.CayGee" />
        <activity
            android:name=".IntentActivity"
            android:exported="false"
            android:label="@string/title_activity_intent"
            android:theme="@style/Theme.CayGee" />
        <activity
            android:name=".LayoutActivity"
            android:exported="false"
            android:label="@string/title_activity_layout"
            android:theme="@style/Theme.CayGee" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.CayGee">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>